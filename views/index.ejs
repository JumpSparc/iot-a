<html ng-app="SolarProject">
<head>
	<title></title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.min.css">
</head>

<body>
	<nav class="navbar navbar-default navbar-static-top" role="navigation">
		<a class="navbar-brand" href="/">Solar Project Test</a>
	</nav>

	<div class="container">
			<div class="header" ng-controller="LogsController as logs">			
				<ul>
					<li ng-repeat="log in data">
						<pre>{{log}}</pre>
					</li>
				</ul>
			</div> 

		<div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
			
		<hr>

			<form name="pushLogs" ng-controller="PushController as push" ng-submit="pushLog(log)">

				<div class="form-group">
					<label for="">id</label>
					<input type="text" ng-model="log.guid" class="form-control">
					
				</div>
				
				<div class="form-group">
					<label for="">generated</label>
					<input type="text" ng-model="log.generated" class="form-control" >	
					
				</div>

				<button type="submit" class="btn btn-primary">Submit</button>
			</form>
		</div>
	</div>

	
	<script src="/socket.io/socket.io.js"></script>
	<script src="/javascripts/angular.min.js"></script>
	<script src="/javascripts/angular-route.min.js"></script>
	<script src="/javascripts/angularApp.js"></script>
	<script src="/javascripts/index/socket-factory.js"></script>
	<script src="/javascripts/index/index-controller.js"></script>
	<script src="/javascripts/index/push-controller.js"></script>
</body>
</html>