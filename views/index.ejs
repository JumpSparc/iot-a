<html ng-app="SolarProject">
<head>
	<title></title>
	<link rel="stylesheet" type="text/css" href="/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="/nvd3/build/nv.d3.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> 
</head>

<body>
	<nav class="navbar navbar-default navbar-static-top" role="navigation">
		<a class="navbar-brand" href="/">Solar Project Test</a>
		<a href="/logout" class="navbar-link navbar-text pull-right">Logout</a>
	</nav>
	<div class="container">
			<div class="header" ng-controller="LogsController as logs">			
				<nvd3 options='options' data='data'></nvd3>

				<div class="col-xs-8 col-sm-8 col-sm-offset-4 col-md-8 col-md-offset-4 col-lg-8 col-lg-offset-4">
					
					<div class=""style="height:200px;overflow-y:auto;">
						<ul>
							<li ng-repeat="log in data[0].values">
									{{"Power:" + log[1] + ", tstamp: " + log[0]}}
							</li>
						</ul>
					</div>
				<br>	
				<br>	
				<br>	
				<br>	
				<br>	
				</div>
			</div> 

	</div>

	
	<script src="/angular/angular.min.js"></script>
	<script src="/angular/angular-route.min.js"></script>
	<script src="/d3/d3.min.js"></script>
	<script src="/nvd3/build/nv.d3.min.js"></script>
	<script src="/angular-nvd3/dist/angular-nvd3.js"></script>
	<script src="/jquery/dist/jquery.min.js"></script>
	<script src="/socket.io/socket.io.js"></script>
	<script src="/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="/javascripts/angularApp.js"></script>
	<script src="/javascripts/index/socket-factory.js"></script>
	<script src="/javascripts/index/index-controller.js"></script>
</body>
</html>